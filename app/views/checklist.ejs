<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title><%= title %> </title>
    <link rel="stylesheet" type="text/css" href="css/display.css" />

</head>
<body>
    <!-- Load the static image file  -->
   
<h2> Symptom Checklist</h2>
<form method="post" action='checklist'>
    <table class="center">
        <thead>
        <tr>
            <th scope="col"> Symptom </th>
           
           
        </tr>
        </thead>
        
        <% symptoms.forEach(function(symptom){ %>
            <tbody>
        <tr>
            <td>
                <div>

                    <input type="checkbox" name="<%= symptom.index %>" value="<%= symptom.index %>" />
                    <label for="checkbox"><%= symptom.name %></label>
  
                </div>

            </td>
    

        </tr>

        <% })%>
    </tbody>
    </table>
    <button type="submit" value="Submit" name="submit">Diagnose</button>

</form>
    
</body>
</html>